{
  "name": "Inbot",
  "version": "0.0.1",
  "description": "A investation simulation bot in discord",
  "main": "dist/index.js",
  "author": "Jeremy",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "cross-env NODE_ENV=development tsnd --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "start": "yarn build && cross-env NODE_ENV=production node ./dist/index.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "discord.js": "^13.8.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.1",
    "tsc-alias": "^1.6.9",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3"
  }
}
